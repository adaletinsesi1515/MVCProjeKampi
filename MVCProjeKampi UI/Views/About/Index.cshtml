@using EntityLayer.Concrete
@model List<About>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Hakkımda Sayfası Veri Listesi</h2>

@Html.Action("AboutPartial", "About")

<br />
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Hakkımda Başlık</th>
        <th>Aktif Yap</th>
        <th>Pasif Yap</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.AboutID</td>
            <td>@item.AboutDetails1</td>
            <td><a href="#" class="btn btn-warning">Aktif</a></td>
            <td><a href="#" class="btn btn-danger">Pasif</a></td>
        </tr>
    }

</table>




